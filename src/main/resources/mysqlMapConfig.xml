<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="cacheEnabled" value="true"/>
        <setting name="defaultStatementTimeout" value="25000"/>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
        <typeAlias type="com.vtest.it.pojo.MesProperties" alias="properties"/>
        <typeAlias type="com.vtest.it.pojo.ComputerBean" alias="computer"/>
    </typeAliases>
    <mappers>
        <mapper resource="mybatisMappers/systemMappers/MesProperties.xml"/>
        <mapper resource="mybatisMappers/systemMappers/MesItemMapper.xml"/>
        <mapper resource="mybatisMappers/systemMappers/RawdataInforMapper.xml"/>
        <mapper resource="mybatisMappers/systemMappers/ComputerMapper.xml"/>
        <mapper resource="mybatisMappers/systemMappers/PropertiesMapper.xml"/>
        <mapper resource="mybatisMappers/systemMappers/DataSourcePath.xml"/>
    </mappers>
</configuration>